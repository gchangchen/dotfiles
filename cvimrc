set localconfig

let barposition = "bottom"
let previousmatchpattern="((?!last)(prev(ious)?|newer|back|«|less|<|‹|上一页| )+)"
let nextmatchpattern="((?!first)(next|older|more|>|›|»|forward|下一页| )+)"

